{
    "name": "atoum/deprecated-extension",
    "type": "library",
    "description": "atoum extension to avoid failures on deprecated errors",
    "keywords": ["TDD","atoum","test","unit testing","deprecated"],
    "homepage": "http://www.atoum.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Gallou",
            "email": "adriengallou@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jubianchi/atoum"
        }
    ],
    "require": {
        "atoum/atoum": "dev-runner-auto-conf@dev"
    },
    "autoload": {
        "psr-4": {
            "mageekguy\\atoum\\deprecated\\": "classes"
        },
        "files": [
            "autoloader.php",
            "configuration.php"
        ]
    }
}
